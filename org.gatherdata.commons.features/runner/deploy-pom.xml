<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <artifactId>gather-commons</artifactId>
    <groupId>org.gatherdata</groupId>
    <version>1.0-SNAPSHOT</version>
  </parent>
  <groupId>org.gatherdata.gather-commons.org.gatherdata.commons.features.build</groupId>
  <artifactId>deployment</artifactId>
  <version>1.0-SNAPSHOT</version>
  <packaging>pom</packaging>
  <name>gather-commons - features</name>
  <description>The common infrastructure for GATHER applications.</description>
  <organization>
    <name>GATHERing</name>
    <url>http://www.gatherdata.org</url>
  </organization>
  <distributionManagement>
    <repository>
      <id>tembo.releases</id>
      <name>Tembo Internal Releases</name>
      <url>http://fatty-server.local.:8081/nexus/content/repositories/releases</url>
    </repository>
    <snapshotRepository>
      <id>tembo.snapshots</id>
      <name>Tembo Internal Snapshots</name>
      <url>http://fatty-server.local.:8081/nexus/content/repositories/snapshots</url>
    </snapshotRepository>
  </distributionManagement>
  <repositories>
    <repository>
      <releases />
      <snapshots />
      <id>central</id>
      <url>http://central</url>
    </repository>
    <repository>
      <id>org.ops4j</id>
      <name>OPS4J Maven Repository</name>
      <url>http://repository.ops4j.org/maven2/</url>
    </repository>
    <repository>
      <releases>
        <enabled>false</enabled>
      </releases>
      <id>ops4j.snapshots</id>
      <url>http://repository.ops4j.org/mvn-snapshots/</url>
    </repository>
  </repositories>
  <properties>
    <org.osgi.service.http.port.secure>8443</org.osgi.service.http.port.secure>
    <eclipselink.version>2.0.0-M6</eclipselink.version>
    <org.osgi.service.http.port>8080</org.osgi.service.http.port>
    <commons-lang.version>2.4</commons-lang.version>
    <commons-codec.version>1.2_1</commons-codec.version>
    <gather-commons.version>1.0-SNAPSHOT</gather-commons.version>
    <aopalliance.version>1.0_1</aopalliance.version>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <geronimo.jta.version>1.1.1</geronimo.jta.version>
    <peaberry.version>1.1.1</peaberry.version>
    <joda-time.version>1.5.2_1</joda-time.version>
  </properties>
</project>
